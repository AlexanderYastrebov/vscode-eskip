{
  "name": "eskip",
  "displayName": "eskip",
  "description": "Skipper eskip file language extension for vscode",
  "version": "0.0.2",
  "publisher": "boopathi",
  "icon": "images/skipper.png",
  "engines": {
    "vscode": "^1.16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boopathi/vscode-eskip"
  },
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "eskip",
        "aliases": [
          "Skipper eskip",
          "eskip"
        ],
        "extensions": [
          ".eskip"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eskip",
        "scopeName": "text.eskip",
        "path": "./syntaxes/eskip.tmLanguage.json"
      }
    ]
  }
}
